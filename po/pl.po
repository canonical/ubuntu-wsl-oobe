msgstr "Brak nazwy użytkownika"

#: ../ubuntu_wsl_oobe/ui/views/identity.py:86
#, python-brace-format
msgid "Username too long, must be less than {limit}"
msgstr "Nazwa użytkownika jest za długa, musi być mniejsza niż {limit}"

#: ../ubuntu_wsl_oobe/ui/views/identity.py:90
msgid "Username must match NAME_REGEX, i.e. [a-z_][a-z0-9_-]*"
msgstr "Nazwa użytkownika może zawierać NAME_REGEX, np. [a-z_][a-z0-9_-]*"

#: ../ubuntu_wsl_oobe/ui/views/identity.py:94
#, python-brace-format
msgid "The username \"{username}\" is reserved for use by the system."
msgstr ""
"Nazwa użytkownika \"{username}\" jest zarezerwowana do użytku przez system."

#: ../ubuntu_wsl_oobe/ui/views/identity.py:99
msgid "Password must be set"
msgstr "Hasło musi być ustawione"

